    "version": "0.2.0",
    "configurations": [
        {
            "name": "Django NO SSL",
            "type": "debugpy",
            "request": "launch",
            "args": [
                "runserver",
                "0.0.0.0:8000"
            ],
            "django": true,
            "autoStartBrowser": false,
            "program": "${workspaceFolder}/manage.py"
        },
        {
            "name": "Django SSL",
            "type": "debugpy",
            "request": "launch",
            "args": [
                "runsslserver",
                "0.0.0.0:8000"
            ],
            "django": true,
            "autoStartBrowser": false,
            "program": "${workspaceFolder}/manage.py"
        },
    ]
}